{
  "name": "youu-admin",
  "version": "1.0.0",
  "description": "A React OOTB Admin Framework",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env PORT=9112 craco start",
    "build": "cross-env craco build",
    "analyze": "cross-env ANALYZE=true craco build",
    "lint": "eslint --ext .js src mock tests",
    "lint:fix": "eslint --fix --ext .js src mock tests",
    "lint-staged": "lint-staged --no-stash",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "ahooks": "^3.1.4",
    "antd": "^4.17.1",
    "classnames": "^2.2.5",
    "css-animation": "^2.0.0",
    "dva": "^2.6.0-beta.22",
    "dva-loading": "^3.0.22",
    "echarts": "^5.1.1",
    "echarts-for-react": "^3.0.1",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.19.3",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.1.0",
    "prop-types": "^15.5.10",
    "qiankun": "^2.3.5",
    "qs": "^6.5.0",
    "react": "^17.0.1",
    "react-document-title": "2.0.3",
    "react-dom": "^17.0.1",
    "react-intl-universal": "^2.4.6",
    "umi-request": "^1.3.0",
    "url-polyfill": "^1.0.10",
    "wangeditor": "^4.5.0",
    "youu-components": "^1.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.0.0",
    "cross-env": "^7.0.0",
    "enzyme": "^3.1.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-markdown": "^2.2.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.5",
    "mocker-api": "^2.7.4",
    "prettier": "^2.2.1",
    "react-scripts": "^5.0.0",
    "webpack-bundle-analyzer": "^4.3.2",
    "webpackbar": "^5.0.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write"
    ],
    "**/*.{js,jsx}": "eslint --ext .js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome > 75"
  ]
}
